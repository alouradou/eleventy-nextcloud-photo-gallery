{
  "name": "eleventy-nextcloud-photo-gallery",
  "version": "0.0.1",
  "description": "A photo gallery starter kit built with the static site generator Eleventy",
  "main": ".eleventy.js",
  "scripts": {
    "build": "npm-run-all build:*",
    "start": "npm-run-all watch:*",
    "dev": "npm-run-all --parallel watch:*",
    "build:eleventy": "npx @11ty/eleventy",
    "watch:eleventy": "npx @11ty/eleventy --serve",
    "build:sass": "sass src/_includes/sass:src/_includes/css",
    "watch:sass": "sass --watch src/_includes/sass:src/_includes/css",
    "debug": "DEBUG=* @11ty/eleventy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alouradou/eleventy-nextcloud-photo-gallery.git"
  },
  "author": {
    "name": "Arthur Louradou",
    "url": "https://github.com/alouradou"
  },
  "keywords": [
    "Photo Gallery",
    "Photo Gallery Template",
    "Eleventy",
    "11ty"
  ],
  "dependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@11ty/eleventy-img": "^4.0.2",
    "@11ty/eleventy-navigation": "^0.3.3",
    "axios": "^1.7.8",
    "clean-css": "^4.2.3",
    "dotenv": "^16.4.5",
    "eleventy-plugin-metagen": "^1.8.3",
    "nextcloud-link": "^1.2.9",
    "npm-run-all": "^4.1.5",
    "sass": "^1.39.0",
    "terser": "^5.7.2",
    "webdav": "^5.7.1"
  }
}
